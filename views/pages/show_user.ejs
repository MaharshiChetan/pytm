<% include ../partials/header.ejs%>
   <div class="container">
      <div class="table-responsive">          
      <table class="table table-hover">
         <thead>
            <tr>
               <th>#</th>
               <th>Firstname</th>
               <th>Lastname</th>
               <th>Email</th>
               <th>Phone Number</th>
               <th>Date</th>
            </tr>
         </thead>
         <tbody>
            <% for(var i = 0; i < Object.keys(users).length; i++) { %>
            <tr>
               <td><%= i + 1 %></td>
               <td><%= users[i].firstname %></td>
               <td><%= users[i].lastname %></td>
               <td><%= users[i].email %></td>
               <td><%= users[i].number %></td>
               <td><%= users[i].created.toGMTString() %></td>
            </tr>
            <% } %>
         </tbody>
      </table>
      </div>
   </div>